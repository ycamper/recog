<?xml version='1.0' encoding='UTF-8'?>
<fingerprints matches="mdns.device-info.txt" protocol="mdns" database_type="util.os">
  <!--
    Fingerprint definitions that are matched against the string values in the
    TXT record returned by an mDNS endpoint for the '_http._tcp.local.'
    domain name (class IN). Note that the host name may need to be prepended
    to the domain name for a server to respond with the record:
    e.g. 'host-name._http._tcp.local'.
  -->

  <fingerprint pattern="^model=DS\d+(\+|\+II|slim|j|xs|se|play|$)">
    <description>Synology Diskstation</description>
    <example>model=DS3617xs</example>
    <param pos="0" name="hw.vendor" value="Synology"/>
    <param pos="0" name="hw.family" value="Diskstation"/>
    <param pos="0" name="hw.product" value="DS"/>
    <param pos="0" name="hw.cpe23" value="cpe:/h:synology:diskstation:-"/>
    <param pos="0" name="hw.device" value="NAS"/>
  </fingerprint>

